{
  "compileOnSave": true,
  "compilerOptions": {
      "outDir": "../",
      "declaration": true,
      "moduleResolution": "node",
      "module": "commonjs",
      "target": "es6",
      "experimentalDecorators": true,
      "noUnusedParameters": true,
      "noUnusedLocals": true,
      "noResolve": false,
      "lib": [
          "es2017","dom"
      ]
  },
  "include": [
      "./**/*"
  ],
  "exclude": [
      "node_modules*",
      "tsconfig.json"
  ]
}